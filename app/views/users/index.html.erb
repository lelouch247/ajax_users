<h2 class='center'>Available Users</h2>
<hr />
<div class="row">
  <div class="col s12 m6">
    <ul id="list_user">
      <% @users.each do |user| %>
      <li
        class="user"
        data-id=<%= user.id %>
        data-name='<%= user.full_name %>'
        >
        <%= user.full_name %> Email: <%= user.email %>
      </li>
      <% end %>
    </ul>
</div>
